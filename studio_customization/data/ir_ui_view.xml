  <record id="studio_customization.default_form_view_fo_52d4c184-432a-46bd-8e44-d5fe0f1d52a5" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <form>
        <header/>
        <sheet string="Bảng theo dõi">
          <div class="oe_title">
            <h1>
              <field name="x_name" required="1" placeholder="Name..."/>
            </h1>
          </div>
          <group name="studio_group_7b4b0d">
            <group name="studio_group_7b4b0d_left"/>
            <group name="studio_group_7b4b0d_right"/>
          </group>
        </sheet>
        <div class="oe_chatter" name="oe_chatter">
          <field name="message_follower_ids"/>
          <field name="message_ids"/>
          <field name="activity_ids"/>
        </div>
      </form>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" eval="False"/>
    <field name="key" eval="False"/>
    <field name="mode">primary</field>
    <field name="model">x_bang_theo_doi</field>
    <field name="name">Default form view for x_bang_theo_doi</field>
    <field name="priority">16</field>
    <field name="type">form</field>
  </record>
